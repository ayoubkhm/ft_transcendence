### Copy this file to `.env` in the auth_service directory and fill in your own values.
# OAuth2 Google credentials (from Google Cloud Console)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
# The URL Google will redirect back to (must match your Google console "Authorized redirect URIs")
CALLBACK_URL=http://localhost:3000/api/auth/google/callback
# Secret string used to sign the session cookie
COOKIE_SECRET=some_random_string